          "datasource": {
            "type": "influxdb",
            "uid": "influxdb"
          },
          "query": "import \"join\"\n\ncpu = from(bucket: \"system_metrics\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"docker_container_cpu\")\n  |> filter(fn: (r) => r[\"_field\"] == \"usage_percent\")\n  |> last()\n  |> group(columns: [\"container_name\"])\n  |> keep(columns: [\"container_name\", \"_value\", \"_time\"])\n  |> rename(columns: {_value: \"CPU %\", container_name: \"Container\"})\n\nmem = from(bucket: \"system_metrics\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"docker_container_mem\")\n  |> filter(fn: (r) => r[\"_field\"] == \"usage\")\n  |> last()\n  |> group(columns: [\"container_name\"])\n  |> keep(columns: [\"container_name\", \"_value\", \"_time\"])\n  |> rename(columns: {_value: \"Memory\", container_name: \"Container\"})\n\njoin.left(\n  left: cpu,\n  right: mem,\n  on: (l, r) => l.Container == r.Container,\n  as: (l, r) => ({Container: l.Container, \"CPU %\": l[\"CPU %\"], Memory: r.Memory})\n)\n  |> drop(columns: [\"_time\"])",
          "refId": "A"
        }
      ],
